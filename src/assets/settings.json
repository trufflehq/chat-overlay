{
    "font": {
        "Open Sans": "open sans, sans-serif",
        "Roboto": "roboto, sans-serif",
        "Arial": "arial, sans-serif",
        "Rubik": "rubik, sans-serif",
        "Impact": "impact, sans-serif",
    },
    "size": {
        "Small": {
            "emoji": {
                "size": "22px",
            },
            "emote": {
                "max-height": "25px",
                "max-width": "75px",
                "right": "-2px"
            },
            "badge": {
                "size": "16px",
                "bottom": "3px",
                "right": "2px"
            },
            "text": {
                "size": "20px",
                "line-height": "30px"
            }
        },
        "Medium": {
            "emoji": {
                "size": "39px",
            },
            "emote": {
                "max-height": "42px",
                "max-width": "128px",
                "right": "-3px"
            },
            "badge": {
                "size": "28px",
                "bottom": "4px",
                "right": "6px"
            },
            "text": {
                "size": "34px",
                "line-height": "55px"
            }
        },
        "Large": {
            "emoji": {
                "size": "55px",
            },
            "emote": {
                "max-height": "60px",
                "max-width": "180px",
                "right": "-4px"
            },
            "badge": {
                "size": "40px",
                "bottom": "8px",
                "right": "5px"
            },
            "text": {
                "size": "40px",
                "line-height": "75px"
            }
        },
    },
    "weight": {
        "Light": "300",
        "Regular": "400",
        "Bold": "700",
        "Bolder": "800"
    },
    "shadow": {
        "None": "none",
        "Small": "drop-shadow(2px 2px 0.2rem black)",
        "Medium": "drop-shadow(2px 2px 0.35rem black)",
        "Large": "drop-shadow(2px 2px 0.5rem black)"
    },
    "textShadow": {
        "None": "none",
        "Small": "2px 2px 2px black",
        "Medium": "2px 2px 4px black",
        "Large": "2px 2px 6px black"
    },
    "stroke": {
        "None": "0px black",
        "Thin": "1px black",
        "Medium": "2px black",
        "Thick": "3px black",
        "Thicker": "4px black"
    },
    "globalEmotes": true
}